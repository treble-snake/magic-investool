{
  "name": "@investool/web-ui",
  "version": "0.11.3",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "bump-version": "yarn version"
  },
  "nx": {
    "namedInputs": {
      "build": [
        "{projectRoot}/**/*",
        "!{projectRoot}/out/**/*",
        "!{projectRoot}/.next/**/*",
        "!{projectRoot}/*.md"
      ]
    },
    "targets": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "inputs": [
          "build",
          "^build"
        ]
      },
      "dev": {
        "dependsOn": [
          "build"
        ]
      },
      "debug": {
        "dependsOn": [
          "build"
        ]
      },
      "start": {
        "dependsOn": [
          "build"
        ]
      },
      "export": {
        "dependsOn": [
          "build"
        ],
        "inputs": [
          "build",
          "^build"
        ]
      }
    }
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@investool/engine": "*",
    "antd": "^5.2.3",
    "millify": "^5.0.1",
    "moment": "^2.29.4",
    "next": "^13.2.3",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.0.4"
  },
  "devDependencies": {
    "@types/node": "18.14.6",
    "@types/ramda": "^0.27.64",
    "@types/react": "^18.0.28",
    "eslint": "^8.34.0",
    "eslint-config-next": "^13.2.3",
    "typescript": "4.9.5"
  }
}
